<%= form_for @seeker do |f| %>
  <p>
    <%= f.label :first_name %>
    <%= f.text_field :first_name %>
    <%= f.label :last_name %>
    <%= f.text_field :last_name %>
  </p>

  <p>
    <%= f.label :photo_url %><br>
    <%= f.text_field :photo_url %>
  </p>
  <%= image_tag @seeker.photo_url %><br>

  <p>
    <%= f.label :bio %><br>
    <%= f.text_area :bio %>
  </p>

  <p>
    <%= f.label :email %>
    <%= f.text_field :email %>
    <%= f.label :phone_number %>
    <%= f.text_field :phone_number %>
  </p>

  <p>
    <%= f.label :city %>
    <%= f.text_field :city %>
    <%= f.label :gender %>
    <%= f.text_field :gender %>
  </p>

  <p>
    <%= f.submit %>
  </p>

<% end %>

<%= link_to "All Seekers", seekers_path %>
