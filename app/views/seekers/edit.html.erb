<h1> Editing Seeker </h1>

<%= form_for :seeker, url: seeker_path(@seeker), method: :patch do |f| %>
  <p>
    <%= f.label :first_name %><br>
    <%= f.text_field :first_name %>
  </p>

  <p>
    <%= f.label :bio %><br>
    <%= f.text_area :bio %>
  </p>

  <p>
    <%= f.submit %>
  </p>

<% end %>

<%= link_to "Back", seekers_path %>
